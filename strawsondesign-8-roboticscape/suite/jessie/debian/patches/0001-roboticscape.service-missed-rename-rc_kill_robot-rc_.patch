From dfe2d2cd88f96b09cb8e6480814e41420bdbbebb Mon Sep 17 00:00:00 2001
From: Robert Nelson <robertcnelson@gmail.com>
Date: Fri, 24 Mar 2017 11:24:41 -0500
Subject: [PATCH] roboticscape.service: missed rename: rc_kill_robot -> rc_kill

fix from Adam Bennett <abennett72@gmail.com> https://groups.google.com/d/msg/beagleboard/rkXxBQC1MnQ/dK61dCuxBwAJ

Signed-off-by: Robert Nelson <robertcnelson@gmail.com>
---
 roboticscape_service/roboticscape.service | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/roboticscape_service/roboticscape.service b/roboticscape_service/roboticscape.service
index f1272f9..c5accdc 100644
--- a/roboticscape_service/roboticscape.service
+++ b/roboticscape_service/roboticscape.service
@@ -7,7 +7,7 @@ User=root
 PIDFile=/var/run/robotcscape.pid
 ExecStartPre=/usr/bin/rc_startup_routine
 ExecStart=/etc/roboticscape/link_to_startup_program
-ExecStop=/usr/bin/rc_kill_robot
+ExecStop=/usr/bin/rc_kill
 
 [Install]
 WantedBy=multi-user.target
